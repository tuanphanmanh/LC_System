@using MyCompanyName.AbpZeroTemplate.Localization
@{
    Layout = "~/Views/Account/_Layout.cshtml";
    ViewBag.DisableTenantChange = true;
}

@section Styles{
    <link href="~/view-resources/Views/Payment/GetPaymentResult.css" rel="stylesheet"/>
}

<div class="login-form align-middle m-0" style="height: 350px">
    <div class="pb-13 pt-lg-0 pt-5">
        <h3 class="fw-bolder text-dark fs-4 fs-1-lg">
            @L("PaymentCouldNotCompleted")
        </h3>
        <div class=" my-5">
            <i class="fas fa-times fa-4x text-danger"></i>
        </div>
        <a class="btn btn-info" href="@Url.Action("Index", "SubscriptionManagement", new {Area = "AppAreaName"})"> @L("GoToSubscriptionManagement")</a>
    </div>
</div>
