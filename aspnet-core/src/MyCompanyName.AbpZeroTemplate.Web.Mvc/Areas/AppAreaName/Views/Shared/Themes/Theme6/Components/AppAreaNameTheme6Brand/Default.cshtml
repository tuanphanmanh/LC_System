@using MyCompanyName.AbpZeroTemplate.Web.Areas.AppAreaName.Models.Layout
@using MyCompanyName.AbpZeroTemplate.Web.Areas.AppAreaName.Views.Shared.Components.AppAreaNameLogo
@model HeaderViewModel
@{
    Layout = null;
}
<a href="@Url.Action("Index", "Home", new {area = string.Empty})" class="d-flex align-items-center">
    @await Component.InvokeAsync(typeof(AppAreaNameLogoViewComponent), new
    {
        LogoSkin = ViewBag.BrandLogoSkin,
        logoClass = "h-45px logo"
    })
</a>
