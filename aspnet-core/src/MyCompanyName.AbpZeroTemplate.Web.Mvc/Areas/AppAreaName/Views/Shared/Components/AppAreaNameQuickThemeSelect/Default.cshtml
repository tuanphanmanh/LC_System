@using Abp.Configuration
@using MyCompanyName.AbpZeroTemplate.Configuration
@model MyCompanyName.AbpZeroTemplate.Web.Areas.AppAreaName.Models.Layout.QuickThemeSelectionViewModel
@if (SettingManager.GetSettingValue<bool>(AppSettings.UserManagement.IsQuickThemeSelectEnabled))
{
    <div id="kt_demo_panel_toggle" class="d-flex align-items-center ms-1 ms-lg-3">
        <div class="@Model.CssClass">
            <i class="@Model.IconClass"></i>
        </div>
    </div>
}
